item: Remark
  Text=Get location of IBM installation manager
end
item: Get Registry Key Value
  Variable=S_IBMINSTMGRLOC
  Key=SOFTWARE\IBM\Installation Manager
  Value Name=location
  Flags=00000100
end
item: Remark
end
item: Remark
  Text=Set config.ini location
end
item: Set Variable
  Variable=S_CONFIGINI
  Value=%S_IBMINSTMGRLOC%\eclipse\configuration\config.ini
end
item: Remark
end
item: Remark
  Text=Read config.ini to get application data location
end
item: Check if File/Dir Exists
  Pathname=%S_CONFIGINI%
  Flags=00000100
end
item: Read/Update Text File
  Variable=S_INIRECORD
  Pathname=%S_CONFIGINI%
end
item: Parse String
  Source=%S_INIRECORD%
  Pattern==
  Variable1=S_INIKEY
  Variable2=S_INIVALUE
end
item: If/While Statement
  Variable=S_INIKEY
  Value=cic.appDataLocation
end
item: Set Variable
  Variable=S_INSTALLEDXML
  Value=%S_INIVALUE%
end
item: End Block
end
item: End Block
end
item: End Block
end
item: Remark
end
item: Remark
  Text=parse string to remove control characters
end
item: Parse String
  Source=%S_INSTALLEDXML%
  Pattern=\:
  Variable1=S_LEFT
  Variable2=S_RIGHT
end
item: Set Variable
  Variable=S_INSTALLEDXML
  Value=%S_LEFT%:%S_RIGHT%
end
item: If/While Statement
  Variable=S_INSTALLEDXML
  Value=\\
  Flags=00010010
end
item: Parse String
  Source=%S_INSTALLEDXML%
  Pattern=\\
  Variable1=S_LEFT
  Variable2=S_RIGHT
end
item: Set Variable
  Variable=S_INSTALLEDXML
  Value=%S_LEFT%\%S_RIGHT%
end
item: End Block
end
item: Remark
